<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ruleset id="9" name="Export2PDF" ver="1" modder="admin.1">
	<rule name="Rule1" id="1">
		<func name="Function1">
			<a ns="com:Datacap.Libraries.FileIO.Actions" name="CopyFile" qi="">
				<p name="sourcefile" type="string" v="@APPPATH(runtime) +\+@BATCHID +\+@ID+.pdf"/>
				<p name="targetfile" type="string" v="@APPPATH(export)+\+@ID+.pdf"/>
				<p name="overwrite" type="bool" v="False"/>
			</a>
		</func>
	</rule>
</ruleset>